pipeline {
    agent any

    stages {
        stage('checkout branch') {
            steps {

                    if ( env.BRANCH_NAME == 'develop') {
                        git checkout -f develop
                    } else if (env.BRANCH_NAME  == 'homolog') {
                        git chechout -f homolog
                    } else {
                        git checkout -f master
                    }

            }
        }
    }
}



