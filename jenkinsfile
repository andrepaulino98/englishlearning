pipeline{
    agent any

    stages {
        stage('checkout branch'){
            steps{
                if(env.BRANCH_NAME == 'develop'){
                    git ccheckout -f develop
                }
                else if(env.BRANCH_NAME == 'homolog'){
                    git ccheckout -f homolog
                }
                else
                    git ccheckout -f master
            }
        }
    }
}
