pipeline {
    agent any

    stages {
        stage('checkout branch') {
          when {
            branch 'master'
          }
            steps { echo "master" }
          when  {
            branch 'develop'
          }
          steps { echo 'develop' }
          when {
            branch 'homolog'
          }
          steps { echo 'homolog' }
        }
    }
}
